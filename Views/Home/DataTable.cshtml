@model IEnumerable<GymProjectMvc.Models.GymModel>

@{
    ViewBag.Title = "DataTable";
}


<div class="container">
    <table class="table  ">
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">MemberName</th>
                <th scope="col">Contact</th>
                <th scope="col">E-Mail</th>
                <th scope="col">Date-Time</th>
                <th scope="col">Address</th>
                <th scope="col">City</th>
            </tr>
        </thead>
        <tbody>

            @foreach (var list in Model)
            {
                <tr>
                    <th scope="row">@Html.DisplayFor(ModelItem => list.id)</th>
                    <td>@Html.DisplayFor(ModelItem => list.memberName) </td>
                    <td>@Html.DisplayFor(ModelItem => list.Contact) </td>
                    <td>@Html.DisplayFor(ModelItem => list.emailId)</td>
                    <td>@Html.DisplayFor(ModelItem => list.startdate)</td>
                    <td>@Html.DisplayFor(ModelItem => list.Address)</td>
                    <td>@Html.DisplayFor(ModelItem => list.City)</td>

                    <td>
                        <a class="btn btn-success" href="/Home/edit?id=@Html.DisplayFor(ModelItem => list.id)">Edit</a>
                        <a class="btn btn-danger" href="/Home/delete?id=@Html.DisplayFor(ModelItem => list.id)">Delete</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
